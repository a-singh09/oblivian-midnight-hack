{
  "name": "@oblivion/sdk",
  "version": "1.0.0",
  "description": "Company SDK for Oblivion Protocol - GDPR compliance that installs like Google Analytics",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "module": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch",
    "prepublishOnly": "npm run build",
    "clean": "rm -rf dist",
    "lint": "echo 'Linting not configured'",
    "docs": "echo 'Documentation generation not configured'"
  },
  "keywords": [
    "gdpr",
    "privacy",
    "compliance",
    "blockchain",
    "midnight",
    "right-to-be-forgotten",
    "data-protection",
    "zero-knowledge",
    "cryptographic-proofs",
    "privacy-preserving"
  ],
  "author": "Oblivion Protocol",
  "license": "MIT",
  "homepage": "https://github.com/oblivion-protocol/sdk#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/oblivion-protocol/sdk.git"
  },
  "bugs": {
    "url": "https://github.com/oblivion-protocol/sdk/issues"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "dependencies": {
    "axios": "^1.5.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "typescript": "^5.0.0",
    "jest": "^29.0.0",
    "@types/jest": "^29.0.0",
    "ts-jest": "^29.0.0"
  },
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE",
    "examples/**/*"
  ],
  "publishConfig": {
    "access": "public"
  }
}
