{
  "name": "@oblivion/contracts",
  "version": "1.0.0",
  "description": "Midnight smart contracts for Oblivion Protocol",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "compile": "compact compile src/DataCommitment.compact managed/DataCommitment && compact compile src/ZKDeletionVerifier.compact managed/ZKDeletionVerifier",
    "build": "npm run compile && tsc",
    "deploy": "node dist/scripts/deploy.js",
    "check-balance": "ts-node scripts/check-balance.ts",
    "test": "jest",
    "test:proof-server": "ts-node scripts/test-proof-server.ts",
    "test:interaction": "tsc && node dist/scripts/test-contract-interaction.js"
  },
  "dependencies": {
    "@midnight-ntwrk/compact-runtime": "^0.9.0",
    "@midnight-ntwrk/ledger": "^4.0.0",
    "@midnight-ntwrk/midnight-js-contracts": "2.0.2",
    "@midnight-ntwrk/midnight-js-http-client-proof-provider": "2.0.2",
    "@midnight-ntwrk/midnight-js-indexer-public-data-provider": "2.0.2",
    "@midnight-ntwrk/midnight-js-level-private-state-provider": "2.0.2",
    "@midnight-ntwrk/midnight-js-network-id": "2.0.2",
    "@midnight-ntwrk/midnight-js-node-zk-config-provider": "2.0.2",
    "@midnight-ntwrk/midnight-js-types": "2.0.2",
    "@midnight-ntwrk/wallet": "5.0.0",
    "@midnight-ntwrk/wallet-api": "5.0.0",
    "@midnight-ntwrk/zswap": "^4.0.0",
    "rxjs": "^7.8.1",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "@types/jest": "^29.0.0",
    "@types/node": "^20.0.0",
    "@types/ws": "^8.5.10",
    "jest": "^29.0.0",
    "ts-node": "^10.9.0",
    "typescript": "^5.0.0"
  }
}
